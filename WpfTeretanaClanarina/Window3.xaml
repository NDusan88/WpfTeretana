                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <Button x:Name="buttonNazad" Content="Nazad" HorizontalAlignment="Left" Margin="368,241,0,0" VerticalAlignment="Top" Width="75" Click="buttonNazad_Click"/>
        <Button x:Name="buttonProduzi" Content="Produzi" HorizontalAlignment="Left" Margin="103,210,0,0" VerticalAlignment="Top" Width="75" Click="buttonProduzi_Click"/>
        <DatePicker Name="dtpVremeDo" HorizontalAlignment="Left" Margin="103,167,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock4" HorizontalAlignment="Left" Margin="21,167,0,0" TextWrapping="Wrap" Text="VremeDo" VerticalAlignment="Top"/>

    </Grid>
</Window>
